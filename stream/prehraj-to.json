{
  "id": "prehraj-to",
  "media": ["movies", "series"],
  "type": ["stream"],
  "name": "Přehraj.to (proxy)",
  "website": "https://blaniok.azurewebsites.net/",
  "repository": "https://raw.githubusercontent.com/Luberda66/streamlet-modules/main/stream/prehraj-to.json",
  "author": "Olbrasoft / Luberda66",
  "version": "2.1.0",
  "description": "Přehraj.to stream provider via Blaniok proxy API (no login required).",
  "baseUrl": "https://blaniok.azurewebsites.net",
  "responseFormat": "json",
  "headers": {
    "accept": "application/json"
  },
  "api": [
    {
      "name": "Vyhledávání (detail)",
      "type": "detail_stream_search",
      "method": "GET",
      "url": "/api/search?q={title}",
      "response": {
        "itemsKey": "movies",
        "mapping": {
          "id": "url",
          "name": "title",
          "type": "video",
          "size": "",
          "ratingUp": "",
          "link": "url"
        }
      }
    },
    {
      "name": "Získání streamu (detail)",
      "type": "detail_stream_link",
      "method": "GET",
      "url": "/api/proxy?url={id}",
      "response": {
        "mapping": {
          "link": "videoUrl"
        }
      }
    }
  ]
}
