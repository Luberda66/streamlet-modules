  "api": [
    {
      "name": "Detail search",
      "type": "detail_stream_search",
      "method": "GET",
      "url": "/posts",
      "response": {
        "itemsKey": "",
        "mapping": {
          "id": "id",
          "name": "title",
          "type": "video",
          "size": "",
          "ratingUp": "",
          "link": "id"
        }
      }
    },
    {
      "name": "Detail link",
      "type": "detail_stream_link",
      "method": "GET",
      "url": "/posts/{id}",
      "response": {
        "mapping": {
          "link": "id"
        }
      }
    }
  ]
