{
  "id": "prehraj-to",
  "media": ["movies", "series"],
  "type": ["stream"],
  "name": "Přehraj.to (proxy)",
  "website": "https://blaniok.azurewebsites.net/",
  "repository": "https://raw.githubusercontent.com/Luberda66/streamlet-modules/main/stream/prehraj-to.json",
  "author": "Olbrasoft / Luberda66",
  "version": "2.0.0",
  "description": "Přehraj.to stream provider via Blaniok proxy API (no login required).",
  "baseUrl": "https://blaniok.azurewebsites.net",
  "responseFormat": "api",

  "api": {
    "search": {
      "path": "/api/search",
      "params": {
        "q": "{title}"
      },
      "results": "movies",
      "mapping": {
        "title": "title",
        "year": "year",
        "poster": "thumbnail",
        "url": "url"
      }
    },

    "stream": {
      "path": "/api/proxy",
      "params": {
        "url": "{item.url}"
      },
      "mapping": {
        "stream": "videoUrl"
      }
    }
  }
}

